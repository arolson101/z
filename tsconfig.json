{
    "version": "1.8",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "outDir": "./temp"
    },
    "filesGlob": [
      "./src/**/*.ts",
      "!./test/**/*.ts",
      "!./node_modules/**/*.ts",
      "!./src/**/*.d.ts"
    ],
		"exclude": [
			"node_modules",
			"test"
		]
}
